#!/bin/sh

{% for k in range(1, NumberofWANwebclients | int +1) %}
docker exec -d clab-{{networkname}}-Client{{k}}-WANweb {{ ipv6_configuration }}
docker exec clab-{{networkname}}-Client{{k}}-WANweb apt-get update -y
docker exec clab-{{networkname}}-Client{{k}}-WANweb apt-get install {{ basic_packages }}
docker exec clab-{{networkname}}-Client{{k}}-WANweb apt-get install macchanger cron -y
docker exec -d clab-{{networkname}}-Client{{k}}-WANweb nohup bash WANweb.sh
{% endfor %}

{% for k in range(1, NumberofWANvpnclients | int +1) %}
docker exec -d clab-{{networkname}}-Client{{k}}-WANvpn {{ ipv6_configuration }}
docker exec clab-{{networkname}}-Client{{k}}-WANvpn apt-get update -y
docker exec clab-{{networkname}}-Client{{k}}-WANvpn apt-get install {{ basic_packages }}
docker exec clab-{{networkname}}-Client{{k}}-WANvpn apt-get install macchanger -y
docker exec -d clab-{{networkname}}-Client{{k}}-WANvpn nohup bash WANvpn.sh
{% endfor %}
